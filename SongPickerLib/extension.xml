<extension xmlns="http://ns.adobe.com/air/extension/3.1">     <id>com.newpixel.air.nativeextensions.SongPicker</id>     <versionNumber>0.0.1</versionNumber>     <platforms>         <platform name="Android-ARM">            <applicationDeployment>                <nativeLibrary>libAndroidSongPicker.jar</nativeLibrary>                <initializer>com.newpixel.air.nativeextensions.SongPickerExtension</initializer>            </applicationDeployment>         </platform>		<platform name="iPhone-ARM">             <applicationDeployment>                 <nativeLibrary>libIOSSongPicker.a</nativeLibrary>                 <initializer>SongPickerExtensionInitializer</initializer>            </applicationDeployment>        </platform>        <platform name="default">            <applicationDeployment/>        </platform>    </platforms> </extension>